<mujoco model="DualArm scene">
    <include file="KIST DualArm-Hand.xml"/>
  
    <compiler angle="radian" coordinate="local" autolimits="true" meshdir="Meshes/" inertiafromgeom="true"/>
	<option timestep="0.001" cone="elliptic" jacobian="auto" tolerance="1e-10" iterations="30" solver="CG" noslip_iterations="30" noslip_tolerance="1e-5"/>
	<option>
		<flag gravity="enable" override="enable" contact="disable"/>
	</option>

	<visual>
		<global fovy="45" linewidth="2" glow="0.3"/>
		<quality shadowsize="4096" numslices="48"/>
		<scale forcewidth="0.05" contactwidth="0.15" contactheight="0.1" com="0.15" jointlength="0.5" jointwidth="0.05"
					 actuatorlength="0.5" actuatorwidth="0.3" framelength="1.5" framewidth="0.05"/>
		<rgba haze="0.6 0.8 0.9 1" force="1 0 0 0.5" com="0 0 1 1"/>
	</visual>

	<default>
		<!-- <geom contype="1" conaffinity="1" condim="3" material="MatLink"/> -->
		<joint limited="false" damping="0.1" armature="0.1" stiffness="0" frictionloss="0.1"/>
		<motor gear="1 0 0 0 0 0" ctrllimited="true" ctrlrange="-100 100"/>	
	</default>
	
	<asset>
		<texture name="texgeom" type="cube" builtin="flat" width="512" height="512" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" mark="cross" markrgb="1 1 1"/>
		<texture name="texplane" type="2d" builtin="checker" width="512" height="512" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" mark="edge" markrgb="1 1 1"/>
		<texture name="sky" type="skybox" builtin="gradient" width="512" height="512" rgb1="1 1 1" rgb2="0.6 0.8 1"/>

		<material name="MatGnd" reflectance="0" specular="1" shininess="0.1" rgba="0.5 0.5 0.5 1"/>
		<material name="MatLink" reflectance="0" specular="1" shininess="0.3" rgba="0.5 0.5 0.5 1"/>
		<material name="MatGeom" texture="texgeom" texuniform="true"/>
		<material name="MatPlane" texture="texplane" reflectance='0.1' texrepeat="0.5 0.5" texuniform="true"/>
		<texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300"/>
		<material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
	</asset>	

    <worldbody>
        <geom name="floor" size="0 0 0.05" type="plane" material="groundplane" condim="3"/>
        
        <!-- <body name = "table" pos = "0.2 0 0.3" euler = "0 0 0" gravcomp="1">
            <geom type = "box" pos = "0 0 0" size="0.1 0.1 0.3" rgba="0.6 0.6 0.6 1"/>
            <!-- <joint type="slide" stiffness="0" damping="0" frictionloss="0" armature="0" range="-1 1"/> -->
        </body> -->

    </worldbody>
  </mujoco>
  